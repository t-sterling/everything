    {
      "type": "table",
      "title": "Entities â€“ Latency & Errors (last 5m)",
      "id": 5,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 10,
        "w": 12,
        "x": 0,
        "y": 6
      },
      "targets": [
        {
          "expr": "topk(15, (sum(rate(mdp_ingest_hydrator_message_total_time_seconds_sum[5m])) by (entity)) / (sum(rate(mdp_ingest_hydrator_message_total_time_seconds_count[5m])) by (entity)))",
          "legendFormat": "{{entity}}",
          "refId": "A"
        },
        {
          "expr": "sum(increase(hydrator_entity_config_error[5m])) by (entity)",
          "legendFormat": "{{entity}} entity_errors",
          "refId": "B"
        }
      ],
      "transformations": [
        {
          "id": "merge",
          "options": {}
