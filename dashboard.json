{
  "uid": "hydrator-health-overview",
  "title": "Hydrator Health Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "style": "dark",
  "tags": [
    "hydrator",
    "mdp",
    "support",
    "management"
  ],
  "templating": {
    "list": [
      {
        "name": "entity",
        "label": "Entity",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "YOUR_PROMETHEUS_UID"
        },
        "refresh": 1,
        "hide": 0,
        "query": "label_values(mdp_ingest_hydrator_message_total_time_seconds_count, entity)",
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "current": {
          "text": "All",
          "value": ".*"
        }
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "Hydrator P95 Total Latency (s)",
      "id": 1,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(mdp_ingest_hydrator_message_total_time_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "textMode": "auto",
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {
              "color": "green",
              "value": null
            },
            {
              "color": "yellow",
              "value": 0.25
            },
            {
              "color": "red",
              "value": 0.5
            }
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Config Error Rate (5m)",
      "id": 2,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(increase(hydrator_entity_config_error[5m]) + increase(hydrator_filter_config_error[5m]) + increase(hydrator_rule_config_error[5m]))",
          "legendFormat": "errors/5m",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "textMode": "auto",
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {
              "color": "green",
              "value": null
            },
            {
              "color": "yellow",
              "value": 10
            },
            {
              "color": "red",
              "value": 100
            }
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Messages Processed /s (5m avg)",
