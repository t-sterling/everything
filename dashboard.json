{
  "uid": "hydrator-health-overview",
  "title": "Hydrator Health Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 2,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "style": "dark",
  "tags": [
    "hydrator",
    "mdp",
    "support",
    "management"
  ],
  "templating": {
    "list": [
      {
        "name": "entity",
        "label": "Entity",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "YOUR_PROMETHEUS_UID"
        },
        "refresh": 1,
        "hide": 0,
        "query": "label_values(mdp_ingest_hydrator_message_total_time_seconds_count, entity)",
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "current": {
          "text": "All",
          "value": ".*"
        }
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "Hydrator Avg Total Latency (s, 5m)",
      "id": 1,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(mdp_ingest_hydrator_message_total_time_seconds_sum[5m])) / sum(rate(mdp_ingest_hydrator_message_total_time_seconds_count[5m]))",
          "legendFormat": "avg_past_5m",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "textMode": "auto",
        "thresholds": {
          "mode": "absolute",
          "steps": [
            { "color": "green", "value": null },
            { "color": "yellow", "value": 0.25 },
            { "color": "red", "value": 0.5 }
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Config Error Count (5m)",
      "id": 2,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(increase(hydrator_entity_config_error_total[5m]) + increase(hydrator_filter_config_error_total[5m]) + increase(hydrator_rule_config_error_total[5m]))",
          "legendFormat": "errors_5m",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "textMode": "auto",
        "thresholds": {
          "mode": "absolute",
          "steps": [
            { "color": "green", "value": null },
            { "color": "yellow", "value": 10 },
            { "color": "red", "value": 100 }
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Messages Processed /s (5m avg)",
      "id": 3,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(mdp_ingest_hydrator_message_total_time_seconds_count[5m]))",
          "legendFormat": "msg_per_s",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "textMode": "auto",
        "thresholds": {
          "mode": "absolute",
          "steps": [
            { "color": "red", "value": null },
            { "color": "yellow", "value": 50 },
            { "color": "green", "value": 100 }
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 1
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Cache Avg Put Latency (s, 5m)",
      "id": 4,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(mdp_ingest_hydrator_cache_put_time_seconds_sum[5m])) / sum(rate(mdp_ingest_hydrator_cache_put_time_seconds_count[5m]))",
          "legendFormat": "avg_past_5m",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "textMode": "auto",
        "thresholds": {
          "mode": "absolute",
          "steps": [
            { "color": "green", "value": null },
            { "color": "yellow", "value": 0.05 },
            { "color": "red", "value": 0.2 }
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3
        },
        "overrides": []
      }
    },
    {
      "type": "table",
      "title": "Entities â€“ Latency & Errors (last 5m)",
      "id": 5,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 10,
        "w": 12,
        "x": 0,
        "y": 6
      },
      "targets": [
        {
          "expr": "sum by (entity) (rate(mdp_ingest_hydrator_message_total_time_seconds_sum[5m])) / sum by (entity) (rate(mdp_ingest_hydrator_message_total_time_seconds_count[5m]))",
          "legendFormat": "{{entity}}",
          "refId": "A"
        },
        {
          "expr": "sum by (entity) (increase(hydrator_entity_config_error_total[5m]))",
          "legendFormat": "{{entity}}_errors",
          "refId": "B"
        }
      ],
      "transformations": [
        {
          "id": "merge",
          "options": {}
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Value",
            "desc": true
          }
        ],
        "footer": {
          "show": false
        },
        "cellHeight": "sm"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.25 },
              { "color": "red", "value": 0.5 }
            ]
          },
          "unit": "s",
          "decimals": 3
        },
        "overrides": [
          {
            "matcher": {
              "id": "byRegexp",
              "options": ".*errors.*"
            },
            "properties": [
              {
                "id": "unit",
                "value": "short"
              },
              {
                "id": "decimals",
                "value": 0
              }
            ]
          }
        ]
      }
    },
    {
      "type": "table",
      "title": "Config Errors by Type (last 5m)",
      "id": 6,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 10,
        "w": 12,
        "x": 12,
        "y": 6
      },
      "targets": [
        {
          "expr": "sum(increase(hydrator_entity_config_error_total[5m]))",
          "legendFormat": "entity_config_error",
          "refId": "A"
        },
        {
          "expr": "sum(increase(hydrator_filter_config_error_total[5m]))",
          "legendFormat": "filter_config_error",
          "refId": "B"
        },
        {
          "expr": "sum(increase(hydrator_rule_config_error_total[5m]))",
          "legendFormat": "rule_config_error",
          "refId": "C"
        }
      ],
      "options": {
        "showHeader": true,
        "cellHeight": "sm"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "gauge",
      "title": "DocDB Avg Latency (s, 5m)",
      "id": 7,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "sum(rate(mdp_ingest_hydrator_docdb_time_seconds_sum[5m])) / sum(rate(mdp_ingest_hydrator_docdb_time_seconds_count[5m]))",
          "legendFormat": "avg_past_5m",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.05 },
              { "color": "red", "value": 0.2 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "gauge",
      "title": "Secondary Cache Avg Put Latency (s, 5m)",
      "id": 8,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 16
      },
      "targets": [
        {
          "expr": "sum(rate(mdp_ingest_hydrator_cahe_put2_time_seconds_sum[5m])) / sum(rate(mdp_ingest_hydrator_cahe_put2_time_seconds_count[5m]))",
          "legendFormat": "avg_past_5m",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.05 },
              { "color": "red", "value": 0.2 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "table",
      "title": "SLA Status by Metric (5m window)",
      "id": 9,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 16
      },
      "targets": [
        {
          "expr": "sum(rate(mdp_ingest_hydrator_message_total_time_seconds_sum[5m])) / sum(rate(mdp_ingest_hydrator_message_total_time_seconds_count[5m]))",
          "legendFormat": "latency_avg_s",
          "refId": "A"
        },
        {
          "expr": "sum(rate(mdp_ingest_hydrator_message_total_time_seconds_count[5m]))",
          "legendFormat": "throughput_msg_per_s",
          "refId": "B"
        },
        {
          "expr": "sum(increase(hydrator_entity_config_error_total[5m]) + increase(hydrator_filter_config_error_total[5m]) + increase(hydrator_rule_config_error_total[5m]))",
          "legendFormat": "config_errors_5m",
          "refId": "C"
        }
      ],
      "options": {
        "showHeader": true,
        "cellHeight": "sm"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "red", "value": 1 }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byRegexp",
              "options": ".*latency.*"
            },
            "properties": [
              { "id": "unit", "value": "s" },
              { "id": "decimals", "value": 3 }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": ".*throughput.*"
            },
            "properties": [
              { "id": "unit", "value": "ops" },
              { "id": "decimals", "value": 1 }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": ".*errors.*"
            },
            "properties": [
              { "id": "unit", "value": "short" },
              { "id": "decimals", "value": 0 }
            ]
          }
        ]
      }
    }
  ]
}
