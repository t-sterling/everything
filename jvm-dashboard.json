{
  "uid": "jvm-health-overview",
  "title": "JVM Health Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "style": "dark",
  "tags": [
    "jvm",
    "spring-boot",
    "support",
    "management"
  ],
  "templating": {
    "list": [
      {
        "name": "job",
        "label": "Job",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "YOUR_PROMETHEUS_UID"
        },
        "refresh": 1,
        "hide": 0,
        "query": "label_values(jvm_memory_used_bytes, job)",
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "current": {
          "text": "All",
          "value": ".*"
        }
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "Heap Usage (%)",
      "id": 1,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "(sum(jvm_memory_used_bytes{job=~\"$job\",area=\"heap\"}) / sum(jvm_memory_max_bytes{job=~\"$job\",area=\"heap\"})) * 100",
          "legendFormat": "heap_used_pct",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "textMode": "auto",
        "thresholds": {
          "mode": "absolute",
          "steps": [
            { "color": "green", "value": null },
            { "color": "yellow", "value": 70 },
            { "color": "red", "value": 90 }
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Non-Heap Usage (%)",
      "id": 2,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "(sum(jvm_memory_used_bytes{job=~\"$job\",area=\"nonheap\"}) / sum(jvm_memory_committed_bytes{job=~\"$job\",area=\"nonheap\"})) * 100",
          "legendFormat": "nonheap_used_pct",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "textMode": "auto",
        "thresholds": {
          "mode": "absolute",
          "steps": [
            { "color": "green", "value": null },
            { "color": "yellow", "value": 70 },
            { "color": "red", "value": 90 }
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Process CPU Usage (%)",
      "id": 3,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "avg(process_cpu_usage{job=~\"$job\"}) * 100",
          "legendFormat": "process_cpu_pct",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "textMode": "auto",
        "thresholds": {
          "mode": "absolute",
          "steps": [
            { "color": "green", "value": null },
            { "color": "yellow", "value": 60 },
            { "color": "red", "value": 85 }
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Uptime (hours)",
      "id": 4,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "max(process_uptime_seconds{job=~\"$job\"}) / 3600",
          "legendFormat": "uptime_h",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "textMode": "auto",
        "thresholds": {
          "mode": "absolute",
          "steps": [
            { "color": "red", "value": null },
            { "color": "yellow", "value": 1 },
            { "color": "green", "value": 24 }
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "h",
          "decimals": 1
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Heap Used vs Max",
      "id": 5,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 5
      },
      "targets": [
        {
          "expr": "sum(jvm_memory_used_bytes{job=~\"$job\",area=\"heap\"})",
          "legendFormat": "used",
          "refId": "A"
        },
        {
          "expr": "sum(jvm_memory_max_bytes{job=~\"$job\",area=\"heap\"})",
          "legendFormat": "max",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": true
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Process CPU Usage (%) over time",
      "id": 6,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 5
      },
      "targets": [
        {
          "expr": "process_cpu_usage{job=~\"$job\"} * 100",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": true
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Live Threads",
      "id": 7,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 13
      },
      "targets": [
        {
          "expr": "jvm_threads_live{job=~\"$job\"}",
          "legendFormat": "{{job}} live",
          "refId": "A"
        },
        {
          "expr": "jvm_threads_peak{job=~\"$job\"}",
          "legendFormat": "{{job}} peak",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": true
        }
      }
    },
    {
      "type": "stat",
      "title": "Avg GC Pause (s, 5m)",
      "id": 8,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 12,
        "y": 13
      },
      "targets": [
        {
          "expr": "rate(jvm_gc_pause_seconds_sum{job=~\"$job\"}[5m]) / rate(jvm_gc_pause_seconds_count{job=~\"$job\"}[5m])",
          "legendFormat": "avg_gc_pause_5m",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "textMode": "auto",
        "thresholds": {
          "mode": "absolute",
          "steps": [
            { "color": "green", "value": null },
            { "color": "yellow", "value": 0.1 },
            { "color": "red", "value": 0.5 }
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "GC Pause Time /s (5m rate)",
      "id": 9,
      "datasource": {
        "type": "prometheus",
        "uid": "YOUR_PROMETHEUS_UID"
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 13
      },
      "targets": [
        {
          "expr": "rate(jvm_gc_pause_seconds_sum{job=~\"$job\"}[5m])",
          "legendFormat": "pause_seconds_per_s",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": true
        }
      }
    }
  ]
}
